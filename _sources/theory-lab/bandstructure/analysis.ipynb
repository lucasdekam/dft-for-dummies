    "import numpy as np\n",
    "import matplotlib.pyplot as plt \n",
    "\n",
    "%matplotlib widget \n",
    "\n",
    "data = np.loadtxt('bands-excel.txt') / 10 - 0.3323\n",
    "bands = np.arange(8) + 1 \n",
    "\n",
    "fig = plt.figure(figsize=(4, 8))\n",
    "ax = fig.add_subplot()\n",
    "\n",
    "for i, kpt in enumerate(data):\n",
    "    plt.plot(bands, kpt, 'k', linewidth=1)"
